@font-face {
  font-family: 'jiangxizhuokai';
  src: url(/static/font/jiangxizhuokai.TTF);
}
.ji {
  background-color: #dd1539;
  min-height: 100vh;
}
.guagua-0 {
  width: 100%;
  height: 500rpx;
}
.guagua-0 image {
  width: 100%;
  height: 100%;
}
.gua-gua-d-top {
  padding-top: 100rpx;
  margin-top: -2rpx;
}
.baocunxiangce {
  font-size: 12px;
  color: #ccc;
  margin-top: 100rpx;
  text-align: center;
}
.an_ne {
  width: 80%;
  margin: 0 auto;
  font-size: 16px;
  padding: 20rpx 0;
  margin-bottom: 50rpx;
  text-align: center;
  color: #FFFFFF;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.lucky {
  width: 100%;
  height: 100vh;
  background-image: url(/static/event/lucky/back.png);
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.lucky .title image {
  width: 436rpx;
  height: 121rpx;
  display: block;
  margin: 0 auto 50rpx;
}
.lucky .turntable {
  width: 650rpx;
  height: 650rpx;
  position: relative;
  margin: 0 auto;
  background-image: url(/static/event/lucky/turntableBack.png);
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
}
.lucky .turntable .gift,
.lucky .turntable .draw {
  width: 172rpx;
  height: 172rpx;
  overflow: hidden;
  position: absolute;
  background: linear-gradient(0deg, #FFE5E5 0%, #FFFFFF 100%);
  box-shadow: 0rpx 2rpx 8rpx 0rpx rgba(0, 0, 0, 0.31);
  border-radius: 10rpx;
}
.lucky .turntable .gift .giftBox {
  position: relative;
}
.lucky .turntable .gift .giftBox image {
  width: 172rpx;
  height: 110rpx;
}
.lucky .turntable .gift .giftBox text {
  display: block;
  margin-top: 12rpx;
  text-align: center;
  font-size: 26rpx;
  font-family: jiangxizhuokai;
  font-weight: 400;
  color: #323232;
}
.lucky .turntable .gift .giftBox .nogift {
  display: block;
  width: 61rpx;
  height: 69rpx;
  margin: 50rpx auto;
  font-size: 30rpx;
  font-family: jiangxizhuokai;
  font-weight: 400;
  color: #323232;
}
.lucky .turntable .gift:nth-child(1) {
  top: 53rpx;
  left: 56rpx;
}
.lucky .turntable .gift:nth-child(2) {
  top: 53rpx;
  left: 239rpx;
}
.lucky .turntable .gift:nth-child(3) {
  top: 53rpx;
  right: 56rpx;
}
.lucky .turntable .gift:nth-child(4) {
  top: 236rpx;
  right: 56rpx;
}
.lucky .turntable .gift:nth-child(5) {
  bottom: 59rpx;
  right: 56rpx;
}
.lucky .turntable .gift:nth-child(6) {
  bottom: 59rpx;
  left: 239rpx;
}
.lucky .turntable .gift:nth-child(7) {
  bottom: 59rpx;
  left: 56rpx;
}
.lucky .turntable .gift:nth-child(8) {
  top: 236rpx;
  left: 56rpx;
}
.lucky .turntable .draw {
  top: 236rpx;
  left: 239rpx;
  background-image: url(/static/event/lucky/draw.png);
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
}
.lucky .turntable .drawing {
  background-image: url(/static/event/lucky/draw_ing.png);
}
.lucky .turntable .giftMask {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.2);
}
.lucky .chances {
  width: 280rpx;
  height: 50rpx;
  margin: 0 auto;
  background: #F0352D;
  border-radius: 0rpx 0rpx 25rpx 25rpx;
  text-align: center;
  line-height: 50rpx;
}
.lucky .chances text {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 800;
  letter-spacing: 5rpx;
  color: #FFFFFF;
}
.lucky .button {
  margin-top: 80rpx;
  margin-bottom: 150rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.lucky .button .btn {
  width: 260rpx;
  height: 80rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 4rpx 9rpx 0rpx rgba(255, 98, 60, 0.6);
  border-radius: 10rpx;
  text-align: center;
  line-height: 80rpx;
}
.lucky .button .btn text {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 800;
}
.lucky .button .rule {
  margin-right: 80rpx;
  color: #FF623C;
}
.lucky .button .record {
  background-color: #FF623C;
  color: #FFFFFF;
}
.lucky .mask {
  width: 100%;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.6);
  position: absolute;
  top: 0;
  left: 0;
}
.lucky .mask .ruleBox,
.lucky .mask .recordBox {
  width: 444rpx;
  height: 582rpx;
  background: #F9C437;
  border-radius: 20rpx;
  margin: 336rpx auto 0;
  padding: 39rpx 48rpx;
}
.lucky .mask .ruleBox .title,
.lucky .mask .recordBox .title {
  margin-bottom: 30rpx;
  display: flex;
  justify-content: center;
}
.lucky .mask .ruleBox .title image,
.lucky .mask .recordBox .title image {
  width: 51rpx;
  height: 44rpx;
}
.lucky .mask .ruleBox .title text,
.lucky .mask .recordBox .title text {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 800;
  color: #FFFFFF;
}
.lucky .mask .ruleBox .textBox,
.lucky .mask .recordBox .textBox {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 800;
  color: #FFFFFF;
  line-height: 35rpx;
}
.lucky .mask .ruleBox .textBox .time,
.lucky .mask .recordBox .textBox .time {
  position: relative;
}
.lucky .mask .ruleBox .textBox .time .point,
.lucky .mask .recordBox .textBox .time .point {
  position: absolute;
  left: -20rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  display: block;
  width: 8rpx;
  height: 8rpx;
  background: #FFFFFF;
  border-radius: 50%;
}
.lucky .mask .getGiftBox {
  width: 500rpx;
  height: 600rpx;
  position: relative;
  background-image: url(/static/event/lucky/redbag.png);
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center;
  margin: 296rpx auto 0;
}
.lucky .mask .getGiftBox .title {
  text-align: center;
  font-size: 30rpx;
  font-family: jiangxizhuokai;
  font-weight: 400;
  color: #323232;
  line-height: 40rpx;
  position: absolute;
  top: 269rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.lucky .mask .getGiftBox image {
  width: 165rpx;
  height: 165rpx;
  position: absolute;
  bottom: 18rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.lucky .mask .close {
  width: 50rpx;
  height: 50rpx;
  margin-top: 32rpx;
  position: relative;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.button-video {
  background-color: red;
