.box-ly-pm {
  margin: 40rpx auto;
  border-radius: 40rpx;
  position: relative;
}
.box-in {
  border-radius: 40rpx;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
/**小圆点 start*/
.dot {
  position: absolute;
  display: block;
  border-radius: 50%;
  height: 20rpx;
  width: 20rpx;
}
.dot:nth-child(odd) {
  background: #fff;
  -webkit-animation: 0.5s odd linear infinite;
          animation: 0.5s odd linear infinite;
}
.dot:nth-child(even) {
  background: #fcf400;
  -webkit-animation: 0.5s even linear infinite;
          animation: 0.5s even linear infinite;
}
