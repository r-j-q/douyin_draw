.animt {
  -webkit-animation: turn 1.2s;
          animation: turn 1.2s;
}
@-webkit-keyframes turn {
0% {
    -webkit-transform: perspective(150px) rotateY(0deg);
            transform: perspective(150px) rotateY(0deg);
}
50% {
    -webkit-transform: perspective(150px) rotateY(0deg);
            transform: perspective(150px) rotateY(0deg);
}
100% {
    -webkit-transform: perspective(150px) rotateY(179.9deg);
            transform: perspective(150px) rotateY(179.9deg);
}
}
