<view><view class="box-ly-pm" style="{{(style_box)}}"><block wx:for="{{dotList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['dot','dot-'+(index+1)]}}"></view></block><view class="box-in" style="{{(style_box_in)}}"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['ct-out','award-'+(index+1),index==indexSelect?'awardSelect':'']}}" style="{{'background-image:'+('url('+(is_img_bg?item.img_bg:'')+')')+';'+('box-shadow:'+('0 8rpx 0 '+box_shadow_Color)+';')}}"><image class="award-image" lazy-load="true" src="{{item.img}}" data-event-opts="{{[['tap',[['toDetailPage',[['o',['index',index]]]]]]]}}" bindtap="__e"></image></view></block><view data-event-opts="{{[['tap',[['start',['$event']]]]]}}" class="{{['btn-start',isRunning?'ative':'']}}" style="{{'box-shadow:'+('0 8rpx 0 '+box_shadow_Color)+';'}}" bindtap="__e">开始</view></view></view></view>