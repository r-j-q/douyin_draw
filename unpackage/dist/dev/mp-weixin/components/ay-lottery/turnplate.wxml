<view><view class="box-ly-zp" style="{{(style_box)}}"><view class="cs-ct" style="{{(style_wh)}}" animation="{{animationData}}"><view class="cs-line"><block wx:for="{{awardsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cs-litem" style="{{'transform:'+('rotate('+item.lineTurn+')')+';'+('background-color:'+(themeColor)+';')+('left:'+(width/2+'rpx')+';')+('height:'+(height/2+'rpx')+';')+('-webkit-transform-origin:'+('50% '+height/2+'rpx')+';')+('transform-origin:'+('50% '+height/2+'rpx')+';')}}"></view></block></view><view class="cs-list"><block wx:for="{{awardsList}}" wx:for-item="iteml" wx:for-index="index2" wx:key="index2"><view class="cs-item" style="{{'color:'+(themeColor)+';'}}"><view class="cs-item-text" style="{{'transform:'+('rotate('+iteml.turn+')')+';'+('-webkit-transform-origin:'+('50% '+height/2+'rpx')+';')+('transform-origin:'+('50% '+height/2+'rpx')+';')}}"><text class="txt">{{iteml.name}}</text><image class="cs-item-text-img" lazy-load="true" src="{{iteml.img}}" data-event-opts="{{[['tap',[['toDetailPage',[['o',['index',index2]]]]]]]}}" bindtap="__e"></image></view></view></block></view></view><view data-event-opts="{{[['tap',[['playReward',['$event']]]]]}}" class="{{['cs-btn',btnDisabled]}}" style="{{'left:'+(width/2-40+'rpx')+';'+('top:'+(height/2-40+'rpx')+';')}}" bindtap="__e">开始</view><view class="cs-btn-table" style="{{'left:'+(width/2-50+'rpx')+';'+('top:'+(height/2+50+'rpx')+';')+('color:'+(themeColor)+';')}}">{{"剩余"+chance_num+"次"}}</view></view></view>